<h1>HOMEPAGE</h1>

    <!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div>

  <div>

    <h2>
      <a href="/post/{{post.id}}">{{post.title}}</a>
    </h2>

    <p>{{posts.user.name}}</p>

  </div>

  <div>
    <p>
      {{post.content}}
    </p>
  </div>

</div>
{{/each}}


